
.list-cell {
	padding: 20rpx;
	border-bottom: 1rpx solid #eeeeee;
}
.cell-top,
.cell-bottom {
	color: #9e9e9e;
}
.cell-top image {
	width: 88rpx;
	height: 88rpx;
	border-radius: 50%;
	vertical-align: middle;
	margin-right: 20rpx;
}
.attention_btn>view:nth-child(1) {
	border-radius: 5px;
	width: 110rpx;
	height: 46rpx;
	font-size: 28rpx;
	background: #f4f4f4;
	color: #000;
	margin-right: 20rpx;
}
.attention_btn>view:nth-child(1) view {
	font-size: 24rpx;
	margin-right: 10rpx;
	vertical-align: middle;
	font-weight: 600;
}
.cell-text {
	font-size: 32rpx;
	line-height: 40rpx;
	color: #000000;
	margin: 15rpx 0 30rpx 0;
}
.cell-imgage {
	position: relative;
}
.cell-imgage image {
	width: 100%;
	border-radius: 20rpx;
}
.cell_play {
	position: absolute;
	font-size: 140rpx;
	color: #FFFFFF;
}
.cell_play_num {
	position: absolute;
	right: 15rpx;
	bottom: 10rpx;
	padding: 1rpx 14rpx;
	font-size: 22rpx;
	border-radius: 40rpx;
	background: rgba(51, 51, 51, 0.7);
	color: #FFFFFF;
}
.cell-bottom {
	margin: 28rpx 0;
}
.cell-bottom view {
	display: inline-block;
	margin: 0 10rpx;
	font-size: 28rpx;
	vertical-align: bottom;
}
.lolStyle {
	color: #9E9E9E;
}
.lolStyle .active,
.lolStyle .active>view {
	color: #fede33;
}

